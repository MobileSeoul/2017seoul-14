<resources>

    <!-- Base application theme. -->
    <style name="SplashTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_image</item>
    </style>
    <style name="WhiteStatus" parent="Theme.AppCompat.Light.NoActionBar">
    </style>
    <style name="GreenStatus" parent="Theme.AppCompat.Light.NoActionBar">
    </style>
    <style name="ToolbarTheme" parent="Base.Widget.AppCompat.Toolbar">
        <item name="android:background">#FFFFFF</item>
    </style>
    <style name="MainTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="ToolbarThemeMenu" parent="Base.Widget.AppCompat.Toolbar">
        <item name="android:background">@color/colorPrimary</item>
    </style>
    <style name="TabLayoutFont" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="MenuLabelsStyle">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
    </style>
    <!--칼린더-->
    <string name="sans_serif" translatable="false">sans-serif</string>
    <!--칼린더-->

</resources>
